{
    "extends": [
      "standard",
      "plugin:vue/essential"],
    "rules": {
      "no-new":0
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "es6": true
    },
    "plugins" : ["vue", "html"],
    "eol-last": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
  }